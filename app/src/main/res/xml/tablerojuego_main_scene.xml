<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/editTextTime"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView2"
            motion:layout_constraintTop_toBottomOf="@+id/textView3" />
        <Constraint
            android:layout_height="389dp"
            android:layout_width="325dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="@+id/tablero"
            motion:layout_constraintStart_toStartOf="@+id/tablero"
            android:id="@+id/imageView"
            motion:layout_editor_absoluteX="44dp"
            motion:layout_editor_absoluteY="168dp" />
        <Constraint
            motion:layout_editor_absoluteY="55dp"
            android:layout_height="639dp"
            motion:layout_editor_absoluteX="0dp"
            android:layout_width="410dp"
            android:id="@+id/tablero" />
        <Constraint
            motion:layout_editor_absoluteY="3dp"
            android:id="@+id/timeRemaining"
            android:layout_width="54dp"
            android:layout_height="45dp"
            motion:layout_constraintStart_toEndOf="@+id/timeDown"
            motion:layout_editor_absoluteX="117dp" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="327dp"
            android:layout_height="387dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="40dp"
            motion:layout_editor_absoluteY="169dp" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="327dp"
            android:layout_height="387dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="40dp"
            motion:layout_editor_absoluteY="169dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>